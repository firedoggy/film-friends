<h1>Sign Up</h1>

<%= form_for(@user, url:'/signup') do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email, required: true %>
    <br> 

        <br>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <br> 

        <br>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <br> 

        <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br> 

        <br>
    <%= f.submit "Submit" %>
<% end %>