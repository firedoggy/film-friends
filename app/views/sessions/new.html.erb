<h1>Login</h1>

<%= flash[:message]  if flash[:message]%>

<%= form_for :user do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br> 

        <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br> 

        <br>
    <%= f.submit "Login" %>
    
<% end %>
Or
<br><%= button_to "Log in with Google", '/auth/google_oauth2'  %>